{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "cache-control": "s-maxage=1, stale-while-revalidate=59"
      },
      "continue": true
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ]
} 